@using Ck_Models;
@model List<Cau2>

<table>
    <thead>
        <tr>
            <th>Tên nhân viên</th>
            <th>Số điện thoại</th>
            <th>Số lần sửa</th>
        </tr>
    </thead>
    <tbody>
        @{
            if(Model != null){
                foreach(var item in Model){
                    <tr>
                        <td>@item.TenNhanVien</td>
                        <td>@item.SoDienThoai</td>
                        <td>@item.SoLanSua</td>
                    </tr>
                }
            }
        }
    </tbody>
</table>